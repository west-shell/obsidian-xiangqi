{
  "name": "obsidian-xiangqi",
  "private": true,
  "version": "2.5.23",
  "description": "Render xiangqi(Chinese chess) positions diagrams in a note.",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "vite build --mode development && node -e \"require('fs').copyFileSync('manifest.json', './test-vault/.obsidian/plugins/xiangqi/manifest.json')\"",
    "build": "vite build --mode production && node -e \"require('fs').copyFileSync('manifest.json', 'build/manifest.json')\"",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest",
    "test:watch": "vitest watch"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "svelte",
    "svelte5",
    "typescript"
  ],
  "author": "weshell",
  "license": "MIT",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^24.3.0",
    "obsidian": "^1.4.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.5.0",
    "vite": "^7.3.0",
    "vitest": "^3.2.4"
  }
}
