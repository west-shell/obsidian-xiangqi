{
  "name": "obsidian-xiangqi",
  "private": true,
  "version": "2.4.4",
  "description": "Render xiangqi(Chinese chess) positions diagrams in a note.",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "vite build --mode development && node -e \"require('fs').copyFileSync('manifest.json', './test-vault/.obsidian/plugins/xiangqi/manifest.json')\"",
    "build": "vite build --mode production && node -e \"require('fs').copyFileSync('manifest.json', 'build/manifest.json')\"",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest",
    "test:watch": "vitest watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "svelte",
    "svelte5",
    "typescript"
  ],
  "author": "weshell",
  "license": "MIT",
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-svelte-csf": "^5.0.6",
    "@storybook/addon-vitest": "^9.0.16",
    "@storybook/svelte-vite": "^9.0.16",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^24.3.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "obsidian": "^1.4.11",
    "playwright": "^1.53.2",
    "storybook": "^9.0.16",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.5.0",
    "vite": "^5.4.19",
    "vitest": "^3.2.4"
  }
}
